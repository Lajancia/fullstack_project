<template>\n  <div>\n    <form @submit.prevent=\"onSubmit\">\n      <p>아이디</p>\n      <input type=\"text\" name=\"id\" v-model=\"id\" />\n      <p>비밀번호</p>\n      <input type=\"password\" name=\"password\" v-model=\"password\" />\n      <p></p>\n      <button>로그인</button>\n    </form>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      id: \"\",\n      password: \"\",\n    };\n  },\n\n  methods: {\n    onSubmit() {\n      const id = this.id;\n      const password = this.password;\n      this.$http\n        .post(\n          \"/api/login\",\n          { id, password },\n          { \"Content-Type\": \"application-json\" }\n        )\n        .then((res) => {\n          if (res.data.user) {\n            this.$store.commit(\"setUser\", res.data.user);\n            this.$router.push({\n              name: \"Home\",\n            });\n          } else if (res.data.message) {\n            alert(res.data.message);\n          }\n        })\n        .catch((err) => {\n          console.error(err);\n        });\n    },\n  },\n};\n</script>\n"],"sourceRoot":""}]}